         report_status = "Report Sent":	 entire_line:
report_status	  <--selected_text
sed -E -n 's/\s*([a-z]+_[a-z]+)\s*=.*Sen.{2}/\1/p'	 <--pattern_1
sed -E -n 's/\s*([a-z]+_[a-z]+)\s*=.{14}.*/\1/p'	 <--pattern_2
sed -E 's/(.+)?(\s*^)\s*?([a-z]+_[a-z]+)\s*?( = )(.*)/\1 \2 XXX \4 \5/'	 <--pattern_3
sed -E -n 's/.*   .*(([a-z]){6}_([a-z]){6})\s* = .*/\1/p'	 <--pattern_4
sed -E -n "s/.{9}([a-z]+_[a-z]+).*/\1/p"	 <--pattern_5
sed -E -n 's/.*^(.+).*=.*/\1/p'	 :pattern_6
