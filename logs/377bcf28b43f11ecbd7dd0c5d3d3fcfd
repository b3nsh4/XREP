         return { "status":report_status,"notes":notes,"Ref:ID":rand_uuid}	 <--entire_line
status	  <--selected_text
sed -E -n 's/ret.{3}.*\"([a-z]+)\":r.{46}/\1/p'	 <--pattern_1
sed -E -n 's/ret.{3}.{4}\s*([a-z]+)\s*\":.{46}.*/\1/p'	 <--pattern_2
sed -E 's/(.+)?(\")\s*?([a-z]+)\s*?(\":r)(.*)/\1 \2 XXX \4 \5/'	 <--pattern_3
sed -E -n 's/.*   .*(([a-z]){6})\s*\":r.*/\1/p'	 <--pattern_4
